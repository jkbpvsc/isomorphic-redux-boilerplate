{
  "name": "isomorphic-redux-boilerplate",
  "version": "1.0.0",
  "description": "A light weight isomorphic redux boilerplate",
  "main": "main.js",
  "scripts": {
    "build": "export NODE_ENV=development && webpack -d",
    "dev": "export NODE_ENV=development && npm run build && nodemon --config nodemon.json --exec babel-node -- src/server/main.js",
    "start": "export NODE_ENV=development && npm run build && babel-node -- src/server/main.js",
    "build:prod": "export NODE_ENV=production && webpack -p",
    "start:prod": "export NODE_ENV=production && npm run build:prod && babel-node -- src/server/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "nodemon": "1.13.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-isomorphic-tools": "^3.0.5"
  },
  "dependencies": {
    "axios": "0.17.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "dev": "^0.1.3",
    "express": "^5.0.0-alpha.6",
    "file-loader": "^1.1.6",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
